{
  "manifest_version": 2,

  "name": "hawtio",
  "description": "Don't cha wish your console was hawt like me?",
  "version": "@VERSION@",

  "icons": {
    "16": "img/logo-16px.png",
    "128": "img/logo-128px.png"
  },

  "permissions": [
    "http://*/"
    /*
     "app.window",
     "storage"
     */
  ],

  "app": {
    "launch": {
      "local_path": "index.html"
    }
  }
}