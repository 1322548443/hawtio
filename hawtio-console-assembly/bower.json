{
  "name": "hawtio-console-assembly",
  "version": "2.0.0",
  "keywords": [
    "hawtio"
  ],
  "main": [
    "dist/hawtio-console-assembly.js",
    "dist/hawtio-console-assembly.css"
  ],
  "ignore": [
    "dist/hawtio-console-assembly-test.js",
    "**/.*",
    "node_modules",
    "libs",
    "plugins"
  ],
  "dependencies": {
    "bootstrap": "~3.3.6",
    "patternfly": "~2.7.0",
    "hawtio-core": "~2.0.0",
    "hawtio-core-navigation": "~2.0.0",
    "hawtio-utilities": "~2.0.0",
    "hawtio-dashboard": "~2.0.6",
    "hawtio-jmx": "~2.0.50",
    "hawtio-integration": "~2.0.3",
    "hawtio-preferences": "~2.0.3",
    "hawtio-template-cache": "~2.0.3"
  },
  "devDependencies": {
    "hawtio-core-dts": "~2.0.0"
  },
  "overrides": {
